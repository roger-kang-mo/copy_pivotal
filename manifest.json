  {
    "name": "Copy Pivotal Link",
    "version": "0.1",
    "description": "Copy the pivotal story ID into the Github PR desciption",
    "manifest_version": 2,
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://github.com/onemedical/*/compare/*"],
        "js": ["copy_link.js"]
      }
    ]
  }
